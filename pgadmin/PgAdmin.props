<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros">
    <pgOut>$(PlatformTarget)\$(PlatformToolset)\$(Configuration)</pgOut>
    <pgOutObj>$(pgOut)\obj</pgOutObj>
  </PropertyGroup>
  <PropertyGroup />
  <ItemDefinitionGroup />
  <ItemGroup>
    <BuildMacro Include="pgOut">
      <Value>$(pgOut)</Value>
    </BuildMacro>
    <BuildMacro Include="pgOutObj">
      <Value>$(pgOutObj)</Value>
    </BuildMacro>
  </ItemGroup>
</Project>